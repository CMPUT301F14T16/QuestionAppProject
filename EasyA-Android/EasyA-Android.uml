                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kEsyPnHUEeSkovSA3hREsQ" name="sortUpVote" specification="_kBH5bnHUEeSkovSA3hREsQ">
                  <ownedParameter xmi:id="_kEsyP3HUEeSkovSA3hREsQ" name="sortOrder">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
                  </ownedParameter>
                  <ownedParameter xmi:id="_kEsyQHHUEeSkovSA3hREsQ" name="questionList" type="_iwfX8XHUEeSkovSA3hREsQ"/>
                  <ownedParameter xmi:id="_kEsyQXHUEeSkovSA3hREsQ" type="_iwfX9XHUEeSkovSA3hREsQ" isOrdered="true" direction="return">
                    <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_kEsyQnHUEeSkovSA3hREsQ" value="*"/>
                  </ownedParameter>
                  <language>java</language>
                  <body>if (sortOrder) {
			Collections.sort(questionList, new Comparator&lt;QuestionList>() {
				public int compare(QuestionList questionList1,
						QuestionList questionList2) {
					// int cmp = a > b ? +1 : a &lt; b ? -1 : 0;
					// compares the 2 upVotes.
					return questionList1.getUpvotes().length() > questionList2
							.getUpvotes().length() ? 1 : Integer
							.parseInt(questionList2.getUpvotes()) &lt; Integer
							.parseInt(questionList1.getUpvotes()) ? -1 : 0;
				}
			});
		} else {
			Collections.sort(questionList, new Comparator&lt;QuestionList>() {
				public int compare(QuestionList questionList1,
						QuestionList questionList2) {
					// int cmp = a > b ? +1 : a &lt; b ? -1 : 0;
					// compares the 2 upVotes.
					return questionList1.getUpvotes().length() > questionList2
							.getUpvotes().length() ? 1 : Integer
							.parseInt(questionList1.getUpvotes()) &lt; Integer
							.parseInt(questionList2.getUpvotes()) ? -1 : 0;
				}
			});
		}

		return questionList;
	</body>
