                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_ue9ql3gREeSXArTGgMeh_g" name="getAllUserFavourites" specification="_uQE6AHgREeSXArTGgMeh_g">
                    <ownedParameter xmi:id="_ue9qmHgREeSXArTGgMeh_g" type="_ubSrgXgREeSXArTGgMeh_g" isOrdered="true" direction="return">
