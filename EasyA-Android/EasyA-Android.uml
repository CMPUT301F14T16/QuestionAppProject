                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kEZQV3HUEeSkovSA3hREsQ" name="getUserById" specification="_kAXrc3HUEeSkovSA3hREsQ">
                    <ownedParameter xmi:id="_kEZQWHHUEeSkovSA3hREsQ" name="id">
                      <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
                    </ownedParameter>
                    <ownedParameter xmi:id="_kEZQWXHUEeSkovSA3hREsQ" type="_iwoh53HUEeSkovSA3hREsQ" direction="return"/>
                    <language>java</language>
                    <body>try {
			return getUserFromCache(id);
		} catch (NoContentAvailableException ex) {
			if (Queue.getInstance().haveInternetConnection()) {
				try {
					ESClient es = new ESClient();
					User u = es.getUserById(id);
					if (u == null)
						throw new NoContentAvailableException();
					SaveSingleUser(u);
					return u;
				} catch (IOException exx) {
					throw new NoContentAvailableException();
				}
			} else {
				throw new NoContentAvailableException();
			}
		} catch (Exception ex) {
			// Catch breach of security access (internet on main queue)
			throw new NoContentAvailableException();
		}
	</body>
                  </ownedBehavior>
                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kEZQWnHUEeSkovSA3hREsQ" name="SaveSingleQuestion" specification="_j9_F33HUEeSkovSA3hREsQ">
                    <ownedParameter xmi:id="_kEZQW3HUEeSkovSA3hREsQ" name="q" type="_iwfX-3HUEeSkovSA3hREsQ"/>
                    <language>java</language>
                    <body>if (q == null)
			return;
		Gson gson = new Gson();
		Type listType = new TypeToken&lt;List&lt;Question>>() {
		}.getType();
		List&lt;Question> aux = gson.fromJson(
				PMDataParser.loadJson(PMFilesEnum.CACHEQUESTIONS), listType);
		if (aux == null)
			aux = new ArrayList&lt;Question>();
		if (aux.contains(q)) {
			int i = aux.indexOf(q);
			aux.remove(i);
			aux.add(i, q);
		} else {
			aux.add(0, q);
		}
		PMDataParser.saveJson(PMFilesEnum.CACHEQUESTIONS, gson.toJson(aux));
	</body>
                  </ownedBehavior>
