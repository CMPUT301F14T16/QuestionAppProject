                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_uzoTMHgREeSXArTGgMeh_g" name="getLocationCoordinates" specification="_uylKJ3gREeSXArTGgMeh_g">
                  <ownedParameter xmi:id="_uzoTMXgREeSXArTGgMeh_g" direction="return">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#double"/>
                    <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_uzoTMngREeSXArTGgMeh_g" value="*"/>
                  </ownedParameter>
                  <language>java</language>
                  <body>PMClient pm = new PMClient();
		switch (pm.getUserLocationPreference()) {
		case OFF:
			return null;
		case SELECT:
			break;
		case GPS:
			if (InternetCheck.haveInternet()
					&amp;&amp; (lastCheck == null || (new Date().getTime()
							- lastCheck.getTime() >= CHECK_PARAMETER))) {
				try {
					double[] aux = findLocation();
					pm.saveUserCoordinates(aux);
					pm.saveUserLocation(GeoCoder.toAdress(aux[0], aux[1]));
					lastCheck = new Date();
				} catch (LocationNotReachableException ex) {
					return new double[] { 0.0, 0.0 };
				}
			}
			break;
		}
		return pm.getUserCoordinates();
	</body>
                </ownedBehavior>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_uzoTM3gREeSXArTGgMeh_g" name="findLocation" specification="_uylKK3gREeSXArTGgMeh_g">
                  <ownedParameter xmi:id="_uzoTNHgREeSXArTGgMeh_g" direction="return">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#double"/>
                    <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_uzoTNXgREeSXArTGgMeh_g" value="*"/>
                  </ownedParameter>
                  <language>java</language>
                  <body>GPSTracker gps = new GPSTracker(ContextProvider.get());
		double[] coordinate = new double[2];
		if (gps.canGetLocation()) {
			coordinate[0] = gps.getLatitude(); // latitude
			coordinate[1] = gps.getLongitude(); // longitude
			return coordinate;
		} else {
			throw new LocationNotReachableException();
		}
	</body>
