                    <ownedParameter xmi:id="_kEZQeHHUEeSkovSA3hREsQ" type="_iwoh53HUEeSkovSA3hREsQ" direction="return"/>
                    <language>java</language>
                    <body>if (Queue.getInstance().haveInternetConnection()) {
			try {
				ESClient es = new ESClient();
				String userId = es.getUserIdByEmail(email);
				if (userId == null)
					throw new NoContentAvailableException();
				User u = es.getUserById(userId);
				if (u == null)
					throw new NoContentAvailableException();
				SaveSingleUser(u);
				return u;
			} catch (IOException ex) {
				throw new NoContentAvailableException();
			}
		} else {
			throw new NoInternetException();
		}
	</body>
