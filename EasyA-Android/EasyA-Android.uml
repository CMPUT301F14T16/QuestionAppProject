                    <ownedParameter xmi:id="_kEZQfXHUEeSkovSA3hREsQ" type="_iwfX9XHUEeSkovSA3hREsQ" isOrdered="true" direction="return">
                      <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_kEZQfnHUEeSkovSA3hREsQ" value="*"/>
                    </ownedParameter>
                    <language>java</language>
                    <body>List&lt;String> userFavs = MainModel.getInstance().getCurrentUser()
				.getFavourites();
		List&lt;QuestionList> aux = getQuestionListFromQuestionsCache();
		if (userFavs == null || userFavs.size() &lt;= 0 || aux == null
				|| aux.size() &lt;= 0)
			throw new NoContentAvailableException();
		for (QuestionList ql : aux) {
			if (!userFavs.contains(ql.getId())) {
				aux.remove(aux.indexOf(ql));
			}
		}
		return aux;
	</body>
