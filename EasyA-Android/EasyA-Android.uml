                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kEZQVHHUEeSkovSA3hREsQ" name="getQuestionById" specification="_kAXrcHHUEeSkovSA3hREsQ">
                    <ownedParameter xmi:id="_kEZQVXHUEeSkovSA3hREsQ" name="id">
