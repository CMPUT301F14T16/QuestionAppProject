                    </ownedParameter>
                    <language>java</language>
                    <body>if (pd != null) {
			pd.dismiss();
		}

		if (result) {
			if (controller.submitedOffline) {
				Toast.makeText(
						ctx,
						&quot;Your reply will be posted online when you connect to the internet!&quot;,
						Toast.LENGTH_LONG).show();
				tv.setText(&quot;&quot;);
			}
