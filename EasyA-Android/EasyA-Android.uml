  <uml:OpaqueBehavior xmi:id="_wBQkiHgREeSXArTGgMeh_g" name="getTimezone" specification="_wBGywHgREeSXArTGgMeh_g">
    <ownedParameter xmi:id="_wBQkiXgREeSXArTGgMeh_g" direction="return">
      <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
    </ownedParameter>
    <language>java</language>
    <body>if (timezone.equals(&quot;&quot;)) {
			Calendar calendar = Calendar.getInstance(
					TimeZone.getTimeZone(&quot;GMT&quot;), Locale.getDefault());
			String timeZone = new SimpleDateFormat(&quot;Z&quot;).format(calendar
					.getTime());
			timezone = &quot;GMT&quot;
					+ (timeZone.substring(0, 3) + &quot;:&quot; + timeZone
							.substring(3, 5));
		}
		return timezone;
	</body>
  </uml:OpaqueBehavior>
