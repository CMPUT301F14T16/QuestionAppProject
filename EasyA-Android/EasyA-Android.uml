                  <body>this.username = username;
		this.user = user;
	</body>
                </ownedBehavior>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_iyHwDXHUEeSkovSA3hREsQ" name="create" specification="_ixh52nHUEeSkovSA3hREsQ">
                  <ownedParameter xmi:id="_iyHwDnHUEeSkovSA3hREsQ" name="username">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
                  </ownedParameter>
                  <ownedParameter xmi:id="_iyHwD3HUEeSkovSA3hREsQ" name="user" type="_iwoh53HUEeSkovSA3hREsQ"/>
                  <ownedParameter xmi:id="_iyHwEHHUEeSkovSA3hREsQ" type="_iu2ZPnHUEeSkovSA3hREsQ" direction="return"/>
                  <language>java</language>
                  <body>return new ChangeUsernameController(username, user);
	</body>
                </ownedBehavior>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_iyHwEXHUEeSkovSA3hREsQ" name="submit" specification="_iu2ZP3HUEeSkovSA3hREsQ">
                  <ownedParameter xmi:id="_iyHwEnHUEeSkovSA3hREsQ" direction="return">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
                  </ownedParameter>
                  <language>java</language>
                  <body>try {
			ESClient es = new ESClient();
			if (es.setUsernameById(this.user.getId(), username)) {
				this.user.setUserName(username);
				MainModel.getInstance().saveMainUser(this.user);
				Cache.getInstance().updateAllUsers();
				return true;
			} else
				return false;
		} catch (IOException ex) {
			return false;
		} catch (Exception ex) {
			return false;
		}
	</body>
                </ownedBehavior>
