                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_iyHwG3HUEeSkovSA3hREsQ" name="MainController" specification="_ixh53nHUEeSkovSA3hREsQ">
                  <ownedParameter xmi:id="_iyHwHHHUEeSkovSA3hREsQ" name="c" type="_iw7c2HHUEeSkovSA3hREsQ"/>
                  <ownedParameter xmi:id="_iyHwHXHUEeSkovSA3hREsQ" type="_iu_jKnHUEeSkovSA3hREsQ" direction="return"/>
