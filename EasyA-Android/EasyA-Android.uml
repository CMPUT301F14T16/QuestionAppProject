                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Tt8IoGa5EeSrof-9yxemKg" name="SaveSingleQuestion" specification="_Ttrp8ma5EeSrof-9yxemKg">
                    <ownedParameter xmi:id="_Tt8IoWa5EeSrof-9yxemKg" name="q" type="_TtLToGa5EeSrof-9yxemKg"/>
                    <language>java</language>
                    <body>Gson gson = new Gson();
		Type listType = new TypeToken&lt;List&lt;Question>>(){}.getType();
		List&lt;Question> aux = gson.fromJson(PMDataParser.loadJson(PMFilesEnum.CACHEQUESTIONS), listType);
		if (aux == null)
			aux = new ArrayList&lt;Question>();
		if (aux.contains(q)){
			int i = aux.indexOf(q);
			aux.remove(i);
			aux.add(i, q);
		}else{
			aux.add(0,q);
		}
		PMDataParser.saveJson(PMFilesEnum.CACHEQUESTIONS, gson.toJson(aux));
		
		//MainActivity.mm.notifyViews();
	</body>
                  </ownedBehavior>
