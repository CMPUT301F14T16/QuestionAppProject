                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_U1tB9ma5EeSrof-9yxemKg" name="GetAllPendings" specification="_Uu4oRGa5EeSrof-9yxemKg">
                  <ownedParameter xmi:id="_U1tB92a5EeSrof-9yxemKg" type="_TtLToma5EeSrof-9yxemKg" isOrdered="true" direction="return">
                    <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_U1tB-Ga5EeSrof-9yxemKg" value="*"/>
                  </ownedParameter>
                  <language>java</language>
                  <body>PMClient pm = new PMClient();
     return pm.getPendings();
