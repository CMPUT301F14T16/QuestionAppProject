                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kEZQdnHUEeSkovSA3hREsQ" name="getUserByEmail" specification="_kAXrfHHUEeSkovSA3hREsQ">
                    <ownedParameter xmi:id="_kEZQd3HUEeSkovSA3hREsQ" name="email">
