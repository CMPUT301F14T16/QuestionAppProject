                  <ownedParameter xmi:id="_ue1uxHgREeSXArTGgMeh_g" type="_uO9fsHgREeSXArTGgMeh_g" direction="return"/>
                  <language>java</language>
                  <body>this.q = q;
	</body>
                </ownedBehavior>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_ue1uxXgREeSXArTGgMeh_g" name="create" specification="_udLh4HgREeSXArTGgMeh_g">
                  <ownedParameter xmi:id="_ue1uxngREeSXArTGgMeh_g" name="q" type="_ubQPQXgREeSXArTGgMeh_g"/>
                  <ownedParameter xmi:id="_ue1ux3gREeSXArTGgMeh_g" type="_uO9fsHgREeSXArTGgMeh_g" direction="return"/>
                  <language>java</language>
                  <body>return new FavouriteController(q);
	</body>
                </ownedBehavior>
