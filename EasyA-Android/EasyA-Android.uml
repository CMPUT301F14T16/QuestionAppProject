                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kEsyJHHUEeSkovSA3hREsQ" name="Queue" specification="_j_LYoXHUEeSkovSA3hREsQ">
                  <ownedParameter xmi:id="_kEsyJXHUEeSkovSA3hREsQ" type="_iw7c2nHUEeSkovSA3hREsQ" direction="return"/>
                  <language>java</language>
                  <body>this.pendings = GetAllPendings();
	</body>
                </ownedBehavior>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kEsyJnHUEeSkovSA3hREsQ" name="getInstance" specification="_j_LYpXHUEeSkovSA3hREsQ">
                  <ownedParameter xmi:id="_kEsyJ3HUEeSkovSA3hREsQ" type="_iw7c2nHUEeSkovSA3hREsQ" direction="return"/>
                  <language>java</language>
                  <body>if (queue == null || (queue != null &amp;&amp; !queue.isAlive())) {
			queue = new Queue();
			queue.start();
		}
		return queue;
	</body>
                </ownedBehavior>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kEsyKHHUEeSkovSA3hREsQ" name="run" specification="_j_LYp3HUEeSkovSA3hREsQ">
                  <language>java</language>
                  <body>while (isActive) {
			try {
				if (!pendings.isEmpty()) {
					if (haveInternetConnection())
						ProcessPendings();
				}
				Thread.sleep(loop_interval);
			} catch (InterruptedException ex) {
			} // Do nothing
			catch (IOException ex) {
				// TODO: deal with IOException or pass it forward
			} catch (Exception ex) {
				// TODO: deal with all other exceptions
				// Currently dealing with them by shutting the Queue System off,
				// may not be the best thing to do
				isActive = false;
				return;
			}
		}
	</body>
                </ownedBehavior>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kEsyKXHUEeSkovSA3hREsQ" name="AddPendingToQueue" specification="_j_LYqnHUEeSkovSA3hREsQ">
