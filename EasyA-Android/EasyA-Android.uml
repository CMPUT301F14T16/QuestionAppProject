                    <ownedParameter xmi:id="_kEZQVnHUEeSkovSA3hREsQ" type="_iwfX-3HUEeSkovSA3hREsQ" direction="return"/>
                    <language>java</language>
                    <body>if (Queue.getInstance().haveInternetConnection()) {
			try {
				ESClient es = new ESClient();
				Question aux = es.getQuestionById(id);
				SaveSingleQuestion(aux);
