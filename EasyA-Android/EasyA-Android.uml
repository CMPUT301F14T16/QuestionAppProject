                    </ownedParameter>
                    <language>java</language>
                    <body>if (result) {
			Intent i = new Intent(ctx, QuestionActivity.class);
			i.putExtra(GeneralHelper.QUESTION_KEY, vp.getName());
			i.addFlags(Intent.FLAG_ACTIVITY_CLEAR_TOP);
			ctx.startActivity(i);
