 				finish();
 				Toast.makeText(getApplicationContext(), &quot;Your answer will be posted online when you connect to the internet!&quot;, Toast.LENGTH_LONG).show();
 			}else{        			
     			String aux = controller.getQuestionId();
     			Intent i = new Intent(ctx,QuestionActivity.class);
     			i.putExtra(MainActivity.QUESTION_KEY, aux);
     			i.addFlags(Intent.FLAG_ACTIVITY_CLEAR_TOP);
     			startActivity(i);
 			}
