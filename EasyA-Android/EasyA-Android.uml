                <ownedAttribute xmi:id="_ix001XHUEeSkovSA3hREsQ" name="pending" visibility="protected" type="_iw7c2HHUEeSkovSA3hREsQ" association="_iybR3XHUEeSkovSA3hREsQ"/>
