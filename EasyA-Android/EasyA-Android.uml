                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Tt7homa5EeSrof-9yxemKg" name="getUserQuestions" specification="_TtrC4Ga5EeSrof-9yxemKg">
                    <ownedParameter xmi:id="_Tt7ho2a5EeSrof-9yxemKg" name="u" type="_TtYH8Ga5EeSrof-9yxemKg"/>
                    <ownedParameter xmi:id="_Tt7hpGa5EeSrof-9yxemKg" type="_TtdAcWa5EeSrof-9yxemKg" isOrdered="true" direction="return">
                      <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_Tt7hpWa5EeSrof-9yxemKg" value="*"/>
                    </ownedParameter>
                    <language>java</language>
                    <body>if (Queue.getInstance().haveInternetConnection()){
			try{
				ESClient es = new ESClient();
				List&lt;QuestionList> lst = es.searchQuestionListsByQuery(&quot;*&quot;, 100);
				List&lt;QuestionList> lstUser = new ArrayList&lt;QuestionList>();
				for (QuestionList q : lst){
					if (q.getUsername().equals(u.getUsername()))
						lstUser.add(q);
				}
				return lstUser;
			}catch(IOException ex){
				return getUserQuestionsFromQuestionsCache(u);
			}
		}else{
			return getUserQuestionsFromQuestionsCache(u);				
		}
	</body>
                  </ownedBehavior>
                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Tt7hpma5EeSrof-9yxemKg" name="getQuestionById" specification="_TsGVkGa5EeSrof-9yxemKg">
