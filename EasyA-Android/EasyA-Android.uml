                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_wBQkQHgREeSXArTGgMeh_g" name="GetAllPendings" specification="_v6f0cngREeSXArTGgMeh_g">
                  <ownedParameter xmi:id="_wBQkQXgREeSXArTGgMeh_g" type="_ubopwHgREeSXArTGgMeh_g" isOrdered="true" direction="return">
                    <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_wBQkQngREeSXArTGgMeh_g" value="*"/>
                  </ownedParameter>
                  <language>java</language>
                  <body>PMClient pm = new PMClient();
		List&lt;Pending> lst = pm.getPendings();
		if (lst == null)
			lst = new ArrayList&lt;Pending>();
		return lst;
	</body>
