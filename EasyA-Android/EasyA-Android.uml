                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_uzoTQngREeSXArTGgMeh_g" name="showSettingsAlert" specification="_uy4FI3gREeSXArTGgMeh_g">
                  <language>java</language>
                  <body>AlertDialog.Builder alertDialog = new AlertDialog.Builder(mContext);

		// Setting Dialog Title
		alertDialog.setTitle(&quot;GPS is settings&quot;);

		// Setting Dialog Message
		alertDialog
				.setMessage(&quot;GPS is not enabled. Do you want to go to settings menu?&quot;);

		// On pressing Settings button
		alertDialog.setPositiveButton(&quot;Settings&quot;,
				new DialogInterface.OnClickListener() {
					public void onClick(DialogInterface dialog, int which) {
						Intent intent = new Intent(
								Settings.ACTION_LOCATION_SOURCE_SETTINGS);
						mContext.startActivity(intent);
					}
				});

		// on pressing cancel button
		alertDialog.setNegativeButton(&quot;Cancel&quot;,
				new DialogInterface.OnClickListener() {
					public void onClick(DialogInterface dialog, int which) {
						dialog.cancel();
					}
				});

		// Showing Alert Message
		alertDialog.show();
	</body>
                </ownedBehavior>
