                <ownedAttribute xmi:id="_uzeiG3gREeSXArTGgMeh_g" name="lastCheck" visibility="private" isStatic="true" type="_ubzo4XgREeSXArTGgMeh_g"/>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_uzoTJXgREeSXArTGgMeh_g" name="checkInternet" specification="_uylKIXgREeSXArTGgMeh_g">
                  <ownedParameter xmi:id="_uzoTJngREeSXArTGgMeh_g" direction="return">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
                  </ownedParameter>
                  <language>java</language>
                  <body>try {
			ConnectivityManager cm = (ConnectivityManager) ContextProvider
					.get().getSystemService(Context.CONNECTIVITY_SERVICE);

			NetworkInfo activeNetwork = cm.getActiveNetworkInfo();
			return activeNetwork != null
					&amp;&amp; activeNetwork.isConnectedOrConnecting();
		} catch (Exception ex) {
			return false;
		}
	</body>
                </ownedBehavior>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_uzoTJ3gREeSXArTGgMeh_g" name="forceCheckForInternet" specification="_uylKIngREeSXArTGgMeh_g">
                  <ownedParameter xmi:id="_uzoTKHgREeSXArTGgMeh_g" direction="return">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
                  </ownedParameter>
                  <language>java</language>
                  <body>lastCheck = null;
		return haveInternet();
	</body>
                </ownedBehavior>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_uzoTKXgREeSXArTGgMeh_g" name="haveInternet" specification="_uylKI3gREeSXArTGgMeh_g">
                  <ownedParameter xmi:id="_uzoTKngREeSXArTGgMeh_g" direction="return">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
                  </ownedParameter>
                  <language>java</language>
                  <body>boolean auxTimer = (lastCheck != null &amp;&amp; ((new Date()).getTime() - lastCheck.getTime()) >= CHECK_FOR_INTERNET);
		if (lastCheck == null || auxTimer) {
			lastCheck = new Date();
			internetStatus = checkInternet();
			if (auxTimer &amp;&amp; internetStatus)
				MainModel.getInstance().notifyViews();
		}
		return internetStatus;
	</body>
                </ownedBehavior>
