                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_wBjev3gREeSXArTGgMeh_g" name="onActivityResult" specification="_v88EtHgREeSXArTGgMeh_g">
                  <ownedParameter xmi:id="_wBjewHgREeSXArTGgMeh_g" name="requestCode">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
