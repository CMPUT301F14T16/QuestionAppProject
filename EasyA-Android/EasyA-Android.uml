                    <body>try{
         	try{
	        		controller = 
	        				NewAnswerController.create(
	        						ctx, 
	        						(getIntent()).getStringExtra(GeneralHelper.AQUESTION_KEY), 
	        						((EditText)findViewById(R.id.submit_answer_body)).getText().toString(), 
     							MainModel.getInstance().getCurrentUser().getId());
