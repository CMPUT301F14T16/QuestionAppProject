                <interfaceRealization xmi:id="_uzeiBHgREeSXArTGgMeh_g" client="_uy4FFXgREeSXArTGgMeh_g" supplier="_uzLnGHgREeSXArTGgMeh_g" contract="_uzLnGHgREeSXArTGgMeh_g"/>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_uzoTNngREeSXArTGgMeh_g" name="GPSTracker" specification="_uy4FFngREeSXArTGgMeh_g">
                  <ownedParameter xmi:id="_uzoTN3gREeSXArTGgMeh_g" name="context" type="_ubL90XgREeSXArTGgMeh_g"/>
                  <ownedParameter xmi:id="_uzoTOHgREeSXArTGgMeh_g" type="_uy4FFXgREeSXArTGgMeh_g" direction="return"/>
