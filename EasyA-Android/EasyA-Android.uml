                  <defaultValue xmi:type="uml:LiteralString" xmi:id="_kEQGInHUEeSkovSA3hREsQ" value="5000"/>
                </ownedAttribute>
                <ownedAttribute xmi:id="_kEQGJHHUEeSkovSA3hREsQ" name="check_for_internet" visibility="package" isLeaf="true" isReadOnly="true">
