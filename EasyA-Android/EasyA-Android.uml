                  <ownedParameter xmi:id="_U1qlwWa5EeSrof-9yxemKg" direction="return">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
                  </ownedParameter>
                  <language>java</language>
                  <body>Random ran = new Random();
		int name = ran.nextInt(5) + 1;
		int number = ran.nextInt(8999) + 1000;
		String aux = &quot;&quot;;
		switch (name) {
		case 1:
			aux = &quot;Guest&quot;;
			break;
		case 2:
			aux = &quot;GreenHorn&quot;;
			break;
		case 3:
			aux = &quot;Inquirer&quot;;
			break;
		case 4:
			aux = &quot;Newcomer&quot;;
			break;
		case 5:
			aux = &quot;Newbie&quot;;
			break;
		default:
			aux = &quot;Guest&quot;;
		}
		return aux + number;
	</body>
                </ownedBehavior>
