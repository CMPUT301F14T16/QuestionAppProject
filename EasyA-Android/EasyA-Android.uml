                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_U1sa-Wa5EeSrof-9yxemKg" name="getInstance" specification="_Uu4BNGa5EeSrof-9yxemKg">
                  <ownedParameter xmi:id="_U1tB8Ga5EeSrof-9yxemKg" type="_TtLTpGa5EeSrof-9yxemKg" direction="return"/>
                  <language>java</language>
                  <body>if (queue == null || (queue != null &amp;&amp; !queue.isAlive())) {
         queue = new Queue();
         queue.start();
     }
 	return queue;
 </body>
                </ownedBehavior>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_U1tB8Wa5EeSrof-9yxemKg" name="run" specification="_Uu4BNma5EeSrof-9yxemKg">
                  <language>java</language>
                  <body>while(isActive)
     {
         try {
             if (!pendings.isEmpty()){
                 if (haveInternetConnection())
                     ProcessPendings();
             }
                 Thread.sleep(loop_interval);
         }
         catch(InterruptedException ex) {} //Do nothing
         catch(IOException ex){
         	//TODO: deal with IOException or pass it forward
         }catch(Exception ex){
             //TODO: deal with all other exceptions
         	//Currently dealing with them by shutting the Queue System off, may not be the best thing to do
             isActive = false;
             return;
         }
     }
 </body>
                </ownedBehavior>
