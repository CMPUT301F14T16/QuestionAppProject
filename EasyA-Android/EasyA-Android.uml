                  <ownedAttribute xmi:id="_ueB2cHgREeSXArTGgMeh_g" name="controller" visibility="private" type="_uO7DcHgREeSXArTGgMeh_g" association="_ufZvcHgREeSXArTGgMeh_g"/>
